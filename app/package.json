    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "^2.0.4",
    "@types/react": "^18.2.6",
    "classnames": "^2.3.2",
    "graphql": "^16.6.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
